    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.3",
    "mysql": "^2.18.1",
    "mysqli": "^3.1.4",
    "pg": "^8.7.1",
    "reflect-metadata": "^0.1.13",
    "tsconfig-paths": "^3.12.0",
    "typeorm": "^0.2.37",
    "typeorm-seeding": "^1.6.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.5",
